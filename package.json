{
  "name": "compare-object-field",
  "version": "1.3.0",
  "description": "Compare an object's field with a value, or filter arrays of objects by a filter object.",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel src -d dist",
    "prepublish": "npm run lint && npm run build",
    "lint": "eslint src/**/*",
    "lint:fix": "eslint --fix src/**/*",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:staged": "jest --findRelatedTests",
    "test:coverage": "jest --coverage"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint",
      "git add",
      "npm run test:staged"
    ]
  },
  "keywords": [
    "compare object field",
    "filter arrays",
    "custom filters"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MarkosKon/compare-object-field.git"
  },
  "author": "AffectionateDoor",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "eslint": "^5.9.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.5.1",
    "husky": "^2.2.0",
    "jest": "^24.7.1",
    "lint-staged": "^8.1.6"
  }
}
